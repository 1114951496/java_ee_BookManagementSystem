<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="user" extends="struts-default">
		<action name="check" class="action.CheckAction">
		<result name="success">/index.jsp</result>
		<result name="error">/error.jsp</result>
		</action>
		<action name="checktype" class="action.CheckAction" method="checktype">
		<result name="success">/index.jsp</result>
		<result name="error">/error.jsp</result>
		</action>
		<action name="back" class="action.BackAction">
		<result name="success">/history.jsp</result>
		<result name="error">/error.jsp</result>
		</action>
		<action name="update" class="action.updateAction">
		<result name="success">/history.jsp</result>
		<result name="error">/error.jsp</result>
		</action>
		<action name="apply" class="action.ApplyAction">
		<result name="success">/index.jsp</result>
		<result name="error">/error.jsp</result>
		</action>
	</package>
</struts>    
